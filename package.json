{
  "name": "api",
  "version": "0.0.1",
  "description": "Backend API for e-commerce",
  "author": {
    "name": "mango",
    "email": "mjnhdi35@gmail.com"
  },
  "private": true,
  "type": "module",
  "license": "MIT",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typecheck": "tsc --noEmit",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f postgres",
    "docker:ps": "docker-compose ps",
    "docker:reset": "docker-compose down -v && docker-compose up -d",
    "migrate:make": "NODE_OPTIONS='--import tsx' knex migrate:make --knexfile knexfile.ts -x ts",
    "migrate:latest": "NODE_OPTIONS='--import tsx' knex migrate:latest --knexfile knexfile.ts",
    "migrate:rollback": "NODE_OPTIONS='--import tsx' knex migrate:rollback --knexfile knexfile.ts",
    "seed:make": "NODE_OPTIONS='--import tsx' knex seed:make --knexfile knexfile.ts -x ts",
    "seed:run": "NODE_OPTIONS='--import tsx' knex seed:run --knexfile knexfile.ts",
    "migrate:prod": "knex migrate:latest --knexfile knexfile.js"
  },
  "dependencies": {
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.3",
    "@nestjs/core": "^11.0.1",
    "@nestjs/platform-express": "^11.0.1",
    "dotenv": "^17.2.4",
    "knex": "^3.1.0",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@types/express": "^5.0.0",
    "@types/node": "^22.10.7",
    "dotenv-expand": "^12.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.21.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  }
}